<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns="http://www.springframework.org/schema/beans"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="questionDao" class="ru.otus.hw.quiz.dao.AnswerDaoCsv">
        <constructor-arg value="/dws/quiz.csv"/>
    </bean>

    <bean id="questionService" class="ru.otus.hw.quiz.service.QuestionServiceImp">
        <constructor-arg name="doa" ref="questionDao"/>
        <constructor-arg name="communicateService" ref="communicateService"/>
    </bean>

    <bean id="communicateService" class="ru.otus.hw.quiz.service.CommunicateServiceImp" init-method="csInit" destroy-method="csDestroy"/>

</beans>